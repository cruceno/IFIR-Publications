@ARTICLE{Eisenmayer201311162,
author={Eisenmayer, T.J. and Lasave, J.A. and Monti, A. and De Groot, H.J.M. and Buda, F.},
title={Proton displacements coupled to primary electron transfer in the Rhodobacter sphaeroides reaction center},
journal={Journal of Physical Chemistry B},
year={2013},
volume={117},
number={38},
pages={11162-11168},
doi={10.1021/jp401195t},
note={cited By 3},
