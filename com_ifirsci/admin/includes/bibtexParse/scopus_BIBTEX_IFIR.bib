abstract={At room temperature, conventionally formed Fe-15Mn-5Si-9Cr-5Ni alloys are normally austenitic and the application of a stress induces a reversible martensitic transformation, causing a shape memory effect (SME). However, when the material is processed by a melt-spinning technique, the rapid solidification can stabilize the high-temperature ferritic phase. We found various austenite/ferrite structures in our melt-spun ribbons. The goals of this work were to correlate the process conditions with the resulting phases, find the appropriate heat treatment in order to recover the equilibrium austenitic phase, characterize the different ribbons formed from this material and evaluate the ribbons' shape memory behaviour. We found that annealing at 1050 °C for 60. min, under a protective argon atmosphere, followed by water quenching, fully retains the austenite to room temperature. We compared particular ribbons, those originally austenitic, to the heat treated ones and analyzed the relationship between yield stress and microstructure and texture. The results from the shape memory tests demonstrate that the ribbons are capable of recovering enough of their shape for certain applications. © 2012 Elsevier B.V.},
author_keywords={Fe-Mn-Si;  Melt-spun;  Shape memory},
keywords={Argon atmospheres;  Austenitic;  Austenitic phase;  Fe-Mn-Si;  Ferritic phase;  High temperature;  Melt-spinning techniques;  Melt-spun;  Melt-spun ribbons;  Process condition;  Reversible martensitic transformation;  Room temperature;  Shape memory;  Shape memory behaviour;  Shape-memory properties;  Water quenching, Austenite;  Martensitic transformations;  Melt spinning;  Microstructure;  Protective atmospheres;  Rapid solidification;  Silicon, Manganese},
document_type={Article},
source={Scopus},
}

@ARTICLE{Sakong20123467,
author={Sakong, S. and Mosch, C. and Lozano, A. and Busnengo, H.F. and Groß, A.},
title={Lowering energy barriers in surface reactions through concerted reaction mechanisms},
journal={ChemPhysChem},
year={2012},
volume={13},
number={15},
pages={3467-3471},
doi={10.1002/cphc.201200526},
note={cited By 6},
url={http://www.scopus.com/inward/record.url?eid=2-s2.0-84867556250&partnerID=40&md5=230d1bbabdf119ff9293650d8a128834},
abstract={Any technologically important chemical reaction typically involves a number of different elementary reaction steps consisting of bond-breaking and bond-making processes. Usually, one assumes that such complex chemical reactions occur in a step-wise fashion where one single bond is made or broken at a time. Using first-principles calculations based on density functional theory we show that the barriers of rate-limiting steps for technologically relevant surface reactions are significantly reduced if concerted reaction mechanisms are taken into account. Concerted reaction mechanisms on surfaces can have significantly lower barriers than the single steps of which the concerted reaction mechanism consists. This is shown by periodic density functional theory calculations for two technologically important reactions, methanol synthesis on copper (see picture) and hydrogen subsurface penetration on palladium. Copyright © 2012 WILEY-VCH Verlag GmbH & Co. KGaA, Weinheim.},
author_keywords={density functional calculations;  heterogeneous catalysis;  hydrocarbons;  reaction mechanisms;  surface chemistry},
document_type={Article},
source={Scopus},
}

@ARTICLE{Lambri2012117,
author={Lambri, O.A. and Pérez-Landazábal, J.I. and Recarte, V. and Cuello, G.J. and Golovin, I.S.},
title={Order controlled dislocations and grain boundary mobility in Fe-Al-Cr alloys},
journal={Journal of Alloys and Compounds},
